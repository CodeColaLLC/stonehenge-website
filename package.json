{
  "name": "webpack-site-starter",
  "version": "1.0.0",
  "description": "A forkable starter project for Webpack-based websites (not apps).",
  "scripts": {
    "build": "NODE_ENV=production webpack -p --config webpack.production.config.js --progress --profile --colors",
    "dev": "webpack-dev-server --progress --profile --colors --hot",
    "deploy": "./deploy.sh"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/CodeColaLLC/webpack-site-starter.git"
  },
  "keywords": [
    "webpack",
    "starter",
    "website",
    "automatic",
    "deployment",
    "php"
  ],
  "author": "CodeCola <dev@codecola.io> (https://codecola.io)",
  "license": "MIT",
  "devDependencies": {
    "babel-core": "6.7.7",
    "babel-loader": "6.2.4",
    "babel-plugin-transform-object-rest-spread": "^6.8.0",
    "babel-preset-es2015": "6.6.0",
    "css-loader": "0.23.1",
    "file-loader": "0.8.5",
    "imports-loader": "0.6.5",
    "style-loader": "0.13.1",
    "url-loader": "0.5.7",
    "webpack": "1.13.0",
    "webpack-dev-server": "1.14.1"
  }
}
